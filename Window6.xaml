        <TextBox x:Name="rezc" HorizontalAlignment="Left" Height="50" Margin="461.8,169.044,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50" Background="#FF101B13" Foreground="White" FontSize="36" FontWeight="Bold" TextAlignment="Center" FontFamily="Times New Roman"/>
