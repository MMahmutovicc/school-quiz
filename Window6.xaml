        <TextBlock HorizontalAlignment="Left" Height="40" Margin="53.8,370.4,0,0" TextWrapping="Wrap" Text="Košarka" VerticalAlignment="Top" Width="150" Foreground="White" FontWeight="Bold" TextAlignment="Center" FontSize="18" FontFamily="Times New Roman"/>
